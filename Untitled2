function findTriangleArea(a,b) {
    return a * b / 2;
}
const area = findTriangleArea(5,2);
console.log(area);
　//三角形の面積を求める、アロー関数に書き換える

const add =(x,y) => {
  const sum = x * y / 2;
  return sum;
};
const result =add(5,2);
console.log(result);

const add = (x,y) => x * y /2;
const result = add(5,2);
console.log(result);

